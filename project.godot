; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Flamecode"
run/main_scene="res://src/scenes/main_menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
run/max_fps=1000
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
config/icon="res://icon.svg"

[autoload]

Main="*res://src/scripts/autoload/main.gd"
InteractionManager="*res://src/scenes/interaction/interaction_manager.tscn"
ItemData="*res://src/scripts/autoload/item_data.gd"
Transition="*res://src/scenes/misc/transition.tscn"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={}
directories/dtl_directory={
"ia_timeline": "res://assets/dialogic/timelines/ia_timeline.dtl",
"mission1_AItimeline": "res://assets/dialogic/timelines/mission1_AItimeline.dtl",
"timeline_test": "res://assets/dialogic/timelines/timeline_test.dtl"
}
text/letter_speed=0.05
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
variables={
"checkPassword": "",
"password": ""
}
layout/style_list=["res://assets/dialogic/styles/dialogue_style.tres"]
layout/default_style="res://assets/dialogic/styles/dialogue_style.tres"
glossary/default_case_sensitive=true
text/autoadvance_fixed_delay=1.0
layout/end_behaviour=0
translation/enabled=true
translation/original_locale="pt_BR"
translation/file_mode=1
translation/translation_folder=""
translation/save_mode=0
translation/add_separator=false
text/autoadvance_enabled=false
text/autocolor_names=true
text/split_at_new_lines=false
portraits/default_portrait=""

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/resizable=false
window/stretch/mode="canvas_items"
window/per_pixel_transparency/allowed=true

[editor]

movie_writer/fps=300

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

